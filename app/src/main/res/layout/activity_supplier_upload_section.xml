<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f4f4f4"
    tools:context=".SupplierUploadSection">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="101dp"
        android:layout_height="34dp"
        android:layout_marginStart="56dp"
        android:textColor="@color/Brown"
        android:layout_marginTop="72dp"
        android:textAlignment="center"
        android:text="Upload Ctizenship"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="101dp"
        android:layout_height="34dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="72dp"
        android:text="Upload  Regsitration "
        android:textAlignment="center"
        android:textColor="@color/Brown"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/uploadRegistration"
        android:layout_width="106dp"
        android:layout_height="33dp"
        android:layout_marginTop="72dp"
        android:background="@color/Gray"
        android:text="None"
        android:textAlignment="center"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.678"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/uploadCitizenship" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="101dp"
        android:layout_height="34dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="108dp"
        android:textColor="@color/Brown"
        android:textAlignment="center"
        android:text="Upload  Photo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/uploadPhoto"
        android:layout_width="106dp"
        android:layout_height="33dp"
        android:layout_marginTop="108dp"
        android:background="@color/Gray"
        android:text="None"
        android:textAlignment="center"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.678"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <info.hoang8f.widget.FButton
        android:id="@+id/UploadButton"
        android:layout_width="164dp"
        android:layout_height="45dp"
        android:shadowColor="@color/Black"
        android:text="Return To Home"
        android:textColor="@color/white"
        app:cornerRadius="4dp"
        android:backgroundTint="@color/medium_dark_color"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.676" />

    <TextView
        android:id="@+id/uploadCitizenship"
        android:layout_width="106dp"
        android:layout_height="33dp"
        android:layout_marginTop="72dp"
        android:background="@color/Gray"
        android:text="None"
        android:textAlignment="center"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.678"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/uploadPhoto" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/photoUpload"
        android:layout_width="55dp"
        android:layout_height="65dp"
        android:layout_marginTop="100dp"
        android:clickable="true"
        android:backgroundTint="@color/BurlyWood"
        android:tint="@color/AliceBlue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.434"
        app:layout_constraintStart_toEndOf="@+id/uploadPhoto"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_upload" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/citizenshipUpload"
        android:layout_width="55dp"
        android:layout_height="58dp"
        android:layout_marginTop="48dp"
        android:clickable="true"
        android:backgroundTint="@color/BurlyWood"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.44"
        app:layout_constraintStart_toEndOf="@+id/uploadCitizenship"
        app:layout_constraintTop_toBottomOf="@+id/photoUpload"
        app:srcCompat="@android:drawable/ic_menu_upload" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/RegistrationUpload"
        android:layout_width="55dp"
        android:layout_height="63dp"
        android:layout_marginTop="48dp"
        android:backgroundTint="@color/BurlyWood"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.445"
        app:layout_constraintStart_toEndOf="@+id/uploadRegistration"
        app:layout_constraintTop_toBottomOf="@+id/citizenshipUpload"
        app:srcCompat="@android:drawable/ic_menu_upload" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="260dp"
        android:layout_height="42dp"
        android:layout_marginTop="32dp"
        android:text="Please Upload the required documents"
        android:textColor="@color/Black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>